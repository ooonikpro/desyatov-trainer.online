---
import Document from "./_Document.astro";

import Navbar from "@components/navbar/Navbar.astro";
import RunningString from "@components/running-string/RunningString.astro";
import Sidebar from "@components/sidebar/Sidebar.astro";

import MainSection from "@blocks/main-section/MainSection.astro";
import Subscriptions from "@blocks/subscriptions/Subscriptions.astro";
import HowItWork from "@blocks/how-it-work/HowItWork.astro";
import Maintenance from "@blocks/maintenance/Maintenance.astro";
import Food from "@blocks/food/Food.astro";
import Faq from "@blocks/faq/Faq.astro";
import About from "@blocks/about/About.astro";
---

<Document>
  <Navbar />
  <MainSection />
  <RunningString />
  <Subscriptions client:visible />
  <RunningString dir="right" />
  <HowItWork client:visible />
  <RunningString />
  <Maintenance client:visible />
  <RunningString dir="right" />
  <Food client:visible />
  <RunningString />
  <Faq client:visible />
  <RunningString dir="right" />
  <About client:visible />
  <RunningString />
  <Sidebar />
</Document>
